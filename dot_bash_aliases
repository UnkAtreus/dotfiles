alias hosts='sudo nano /etc/hosts'
alias http-server="python -m http.server"
alias adb_set_proxy="adb shell settings put global http_proxy $(ip -o -4 addr show enp7s0|awk '{print $4}' | sed 's/\/.*//g'):8080"
alias adb_unset_proxy="adb shell settings put global http_proxy :0"

